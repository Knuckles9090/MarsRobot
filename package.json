{
  "name": "marsrobot",
  "version": "1.0.0",
  "description": "Example app for Runpath",
  "main": "index.html",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node server/server.js",
    "postinstall": "npm run bower-up && npm run prestart && npm run update-webdriver && chmod a+x server/server.js",
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update",
    "bower-up": "./node_modules/.bin/bower install",
    "build": "grunt create-production",
    "e2e-tests": "./node_modules/.bin/protractor client/tests/protractor.conf.js",
    "unit-tests": "./node_modules/.bin/karma start client/tests/karma.conf.js",
    "watch-files": "./node_modules/.bin/nodemon",
    "grunt-default": "./node_modules/.bin/grunt default"
  },
  "keywords": [
    "AngularJS"
  ],
  "devDependencies": {
    "bower": "^1.7.7",
    "grunt": "~0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-less": "^1.2.0",
    "grunt-contrib-uglify": "~0.5.0",
    "grunt-ng-annotate": "^1.0.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.7",
    "less": "^2.5.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "nodemon": "^1.8.1",
    "protractor": "^3.1.1"
  },
  "author": "Joona Rahko",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Knuckles9090/ImageWallApp.git"
  },
  "bugs": {
    "url": "https://github.com/Knuckles9090/ImageWallApp/issues"
  },
  "homepage": "https://github.com/Knuckles9090/ImageWallApp#readme"
}
